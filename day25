
CREATE TABLE PRODUCT(
NUM INT PRIMARY KEY AUTO_INCREMENT,
NAME VARCHAR(100) NOT NULL,
PRICE INT NOT NULL,
DISCOUNT INT NOT NULL
);

DROP TABLES PRODUCT;

------- JDBC 쿼리
DELETE FROM PRODUCT WHERE NUM ="+pVO.getNum()+";
UPDATE PRODUCT SET NAME = "+pVO.getNAME()+" WHERE NUM = "+pVO.getNum()+";

SELECT NUM, NAME, PRICE, DISCOUNT FROM PRODUCT WHERE NAME LIKE '%"+pVO.getNAME()+"%';
